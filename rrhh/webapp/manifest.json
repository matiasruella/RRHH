{
  "_version": "1.40.0",
  "sap.app": {
    "id": "mr.rrhh",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap-ux/fiori-freestyle-writer:basic",
      "version": "0.11.15",
      "toolsId": "2ff91bb4-04a5-4236-8c38-626d5ae6769c"
    },
    "dataSources": {
      "ZEMPLOYEES_SRV": {
        "uri": "/sap/opu/odata/sap/ZEMPLOYEES_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "mr.rrhh.i18n.i18n"
        }
      },
      "employeeModel": {
        "dataSource": "ZEMPLOYEES_SRV",
        "type": "sap.ui.model.odata.v2.ODataModel",
        "preload": true,
        "settings": {
            "defaultOperationMode":"Server",
            "defaultBindingMode": "TwoWay",
            "defaultCountMode":"Request",
            "refreshAfterChange":"false",
            "useBatch":false,
            "defaultUpdateMethod":"Put"  
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "mr.rrhh.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": "",
          "target": [
            "TargetMain"
          ]
        },
        {
            "name":"RouteEmployee",
            "pattern":"Employee",
            "target":[
                "TargetEmployee"
            ]
        },
        {
            "name":"RouteMonitor",
            "pattern":"Monitor",
            "target":[
                "TargetMonitor"
            ]
        },
        {
            "name":"RouteOrders",
            "pattern":"Orders",
            "target":[
                "TargetOrders"
            ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "TargetEmployee": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Employee",
          "viewName": "Employee"
        },
        "TargetMonitor": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Monitor",
          "viewName": "Monitor"
        },
        "TargetOrders": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Orders",
          "viewName": "Orders"
        }

      }
    },
    "rootView": {
      "viewName": "mr.rrhh.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    }
  }
}
